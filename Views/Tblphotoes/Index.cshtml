@model IEnumerable<WebApplication5.Models.Tblphoto>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Photourl)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Category)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Customer)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Photographer)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Photourl)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Category.Categoryid)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Customer.Customerid)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Photographer.Photographerid)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Photoid">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Photoid">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Photoid">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>

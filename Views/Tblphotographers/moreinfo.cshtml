
@model WebApplication5.Models.Tblphotographer

@{
    ViewData["Title"] = "moreinfo";
}
<section id="exhibition">
    <div class="container">
        <div class="row">
            <div class="col-sm-8">
                <div class="exhibition_new clearfix">
                    <div class="col-sm-4 space_left">
                        <div class="exhibition_new_l clearfix">
                            <img src="~/img/@Html.DisplayFor(model => model.Phprofile)" style="border-radius:50%" height="100px" width="100px"><span> <h3> @Html.DisplayFor(model => model.Photographername)</h3></span>
                            <h4>@Html.DisplayFor(model => model.City.Cityname)</h4>
                           
                            <h4></h4>
                            <ul class="social_tag">
                                <li><a href="#">Book</a></li>
                            </ul>
                        </div>
                    </div>
                    <div class="col-sm-4 space_all">
                        <div class="exhibition_new_r clearfix">


                            <div class="exhibition_new_rr clearfix">
                                <p id="map" style="height:200px;width:200px;"></p>
                                <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAjHc0bWARHQ6v0UqQXoQywYqOUnaRfQHU&callback=initMap"
                                        defer></script>
                                <script>
                                    let map;
                                    function initMap() {
                                        navigator.geolocation.getCurrentPosition(showPosition);
                                        function showPosition(position) {
                                            map = new google.maps.Map(document.getElementById('map'), {
                                                center: { lat: @Html.DisplayFor(model => model.Lat), lng: @Html.DisplayFor(model => model.Lan) },
                                                zoom: 8,
                                            });
                                        }
                                    }

                                </script>
                            </div>

                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
<br />
<br />
<section id="connect" class="gallery_page">
    <div class="container">
        <div class="row">
            <div class="connect clearfix">
                <h2>GALLERY</h2>
            </div>
            <div class="connect_1 clearfix">
                <div class="gallery">
                    @{
                        var data = (Tblphoto)ViewData["Message"];
                        <p>@data</p>
                    }
                    <a href="~/img/gallery/@data.Photourl" class=""><img src="~/img/gallery/@data.Photourl" alt="" height="100px" width="100px"></a>

                </div>
            </div>
        </div>
    </div>
</section>
<br />
<br />